pub(crate) enum Frame {
    Stream,
    ResetStream,
}

const FIN: u8 = 0;

impl Frame {
    pub fn decode(&self, buf: &mut [u8]) {
        todo!()
    }

    pub fn close_stream(&self, buf: &mut [u8]) {
        // set FIN bit
        self.decode(buf);
        buf[buf.len() - 1] = FIN;
        todo!()
    }
}
